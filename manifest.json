{
    "manifest_version": 3,
    "name": "HTML Fetcher",
    "version": "1.0",
    "description": "Fetch HTML code of a web page and send it to a Python script",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
